node_param_inst:
  _1 = NODE n = ident _3 = EQUALS s = ident p = tlist_LPARAMBRACKET_SEMICOLON_RPARAMBRACKET_node_call_static_param_ _6 = SEMICOLON
    {    ( (n, s, p) )}

node_sep:
  _1 = DOT
    {                          ( )}
| _1 = SEMICOLON
    {                          ( )}

contract:
