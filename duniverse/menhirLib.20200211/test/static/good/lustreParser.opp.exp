  _1 = VAR l = nonempty_list_var_decl_
    {                                     ( List.flatten l )}

var_decl:
  l = clocked_typed_idents _2 = SEMICOLON
    {                                        ( l )}

node_equation:
  _1 = ASSERT e = expr _3 = SEMICOLON
    {let _startpos = _startpos__1_ in
    ( A.Assert (mk_pos _startpos, e) )}
| l = left_side _2 = EQUALS e = expr _4 = SEMICOLON
