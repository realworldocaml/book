      ( Variable.register x )}

maybe_qualified_datacon_:
  datacon = UIDENT
    {let x =     ( Datacon.register datacon ) in
    ( Unqualified x )}
| name = LIDENT _2 = COLONCOLON datacon = UIDENT
    {let x =     ( Datacon.register datacon ) in
let m =     ( Module.register name ) in
    ( Qualified (m, x) )}

maybe_qualified_variable_:
  x = variable
    {    ( Unqualified x )}
| name = LIDENT _2 = COLONCOLON x = variable
    {let m =     ( Module.register name ) in
    ( Qualified (m, x) )}

