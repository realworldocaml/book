| x = raw_consumes_type _2 = STAR x_inlined1 = raw_very_loose_type
    {let ty2 =
  let (_endpos_x_, _startpos_x_, x) = (_endpos_x_inlined1_, _startpos_x_inlined1_, x_inlined1) in
