%start main
%token LPAR
%token NEW_LINE
%token RPAR
%token UNIT
%type <unit> main
%%

main:
  e = expr_or_assign_empty_
    {                                ( e )}

expr_or_assign_cr_:
  e = expr_cr_
    {                            ( e )}

expr_or_assign_empty_:
  e = expr_empty_
    {                            ( e )}

expr_or_assign_cr:
  e = expr_or_assign_cr_
    {                            ( e )}

expr_cr_:
  _1 = cr p = LPAR e = expr_or_assign_cr _4 = cr _5 = RPAR
    {                                                      ( e )}
| e = UNIT
    {                                                        ( e )}

expr_empty_:
  _1 = empty p = LPAR e = expr_or_assign_cr _4 = cr _5 = RPAR
    {                                                      ( e )}
| e = UNIT
    {                                                        ( e )}

cr:
  _1 = NEW_LINE _2 = cr
    {                ( )}
| 
    {                ( )}

empty:
  
    {                ( )}

%%




