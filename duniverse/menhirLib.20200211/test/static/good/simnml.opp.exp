Location:
  _1 = ID
    {  ( if (Sem.is_location _1) || (Sem.is_loc_spe _1) || (Sem.is_loc_mode _1)
			then	Irg.LOC_REF (Sem.get_loc_ref_type _1, _1, Irg.NONE, Irg.NONE, Irg.NONE)
