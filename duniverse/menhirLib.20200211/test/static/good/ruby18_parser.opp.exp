    {                    ( state.cmdarg_stack <- Stack_state.of_list _1;
                      _2 )}

command_args_e1:
  
    {                    ( let list = Stack_state.to_list state.cmdarg_stack in
                        Stack_state.push state.cmdarg_stack true;
                        list )}

open_args:
  _1 = call_args
    {                    ( _1 )}
| _1 = LPAREN_ARG _2 = open_args_e1 _3 = RPAREN
