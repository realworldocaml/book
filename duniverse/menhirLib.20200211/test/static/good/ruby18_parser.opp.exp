| _1 = K_DEF _2 = singleton _3 = dot_or_colon _4 = primary_e9 _5 = fname _6 = primary_e10 _7 = f_arglist _8 = bodystmt _9 = K_END
    {                    ( let ret = Defs (_2, fst _5, _7, _8, annot _1) in
