| _1 = LPAREN_ARG _2 = call_args2 _3 = open_args_e1 _4 = RPAREN
    {                    ( warning "don't put space before argument parentheses";
                      _2 )}

open_args_e1:
  
    {                  ( state.lex_state <- Expr_endarg )}

block_arg:
