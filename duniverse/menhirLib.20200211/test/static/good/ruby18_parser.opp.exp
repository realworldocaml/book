                        ret )}
| _1 = K_CLASS _2 = LSHFT _3 = expr _4 = primary_e5 _5 = term _6 = primary_e6 _7 = bodystmt _8 = K_END
    {                    ( let ret = Sclass (_3, _7, annot _1) in
                        state.in_def <- _4;
                        state.in_single <- _6;
