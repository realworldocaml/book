| _1 = arg _2 = GT _3 = arg
    {                    ( new_call _1 ">" [Arg_value _3] ~annot:(annot _2) )}
| _1 = arg _2 = GEQ _3 = arg
    {                    ( new_call _1 ">=" [Arg_value _3] ~annot:(annot _2) )}
| _1 = arg _2 = LT _3 = arg
