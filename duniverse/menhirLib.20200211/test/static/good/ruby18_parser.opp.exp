| _1 = args _2 = opt_block_arg
    {                    ( _1 @ _2 )}
| _1 = args _2 = COMMA _3 = STAR _4 = arg_value _5 = opt_block_arg
    {                    ( _1 @ (Arg_splat _4 :: _5) )}
| _1 = assocs _2 = opt_block_arg
    {                    ( Arg_hash _1 :: _2 )}
| _1 = assocs _2 = COMMA _3 = STAR _4 = arg_value _5 = opt_block_arg
