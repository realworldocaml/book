atomicPattern:
  _1 = quoteExpr
    {      ( Pat_expr(_1,lhs()) )}
| _1 = CHAR _2 = DOT_DOT _3 = CHAR
    {                      ( Pat_range (_1,_3,rhs2 1 3) )}
| _1 = LBRACE _2 = recordPatternElements _3 = RBRACE
