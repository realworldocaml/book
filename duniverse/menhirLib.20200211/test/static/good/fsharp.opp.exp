| _1 = MINUS _2 = INT32
    {                ( Pat_const (Const_int32 (Int32.neg _2),lhs()) )}
| _1 = ADJACENT_PREFIX_PLUS_MINUS_OP _2 = INT32
    {      ( match _1 with
        | "-" -> Pat_const (Const_int32 (Int32.neg _2),lhs())
