| 
    {                                ( None )}

limit_expr:
  _1 = LOCALS _2 = Int
    {                             ( limit_locals := _2 )}
| _1 = STACK _2 = Int
    {                            ( limit_stack := _2 )}
| _1 = Word _2 = Int
    {                           ( () )}

line_expr:
  _1 = Int
    {                      ( lines := (!pos, _1) :: !lines )}

throws_expr:
  _1 = classname
    {                            ( throws := _1 :: !throws )}

catch_expr:
  _1 = classname _2 = FROM _3 = Word _4 = TO _5 = Word _6 = USING _7 = Word
    {                  (
                    excs := {
                      JCode.e_start = label2int _3;
                      e_end = label2int _5;
                      e_handler = label2int _7;
                      e_catch_type = Some _1
                    } :: !excs
                  )}
| _1 = classname _2 = FROM _3 = Int _4 = TO _5 = Int _6 = USING _7 = Int
    {                  (
                    excs := {
                      JCode.e_start = _3;
                      e_end = _5;
                      e_handler = _7;
                      e_catch_type = Some _1
                    } :: !excs
                  )}

set_expr:
  _1 = Word _2 = any_item
    {                                ( () )}

stackmap:
