stack_item_expr:
  _1 = LOCALS _2 = Word
    {                    ( "" )}
| _1 = LOCALS _2 = Word _3 = Word
    {                    ( "" )}
| _1 = LOCALS _2 = Word _3 = Int
    {                    ( "" )}
