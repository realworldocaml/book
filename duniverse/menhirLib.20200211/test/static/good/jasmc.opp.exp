    {                  ( () )}
| _1 = USE _2 = defstack_same _3 = stack_map_frame_desc _4 = endstack
    {                  ( () )}

defstack_same:
  _1 = defstack_same_expr _2 = LOCALS _3 = SEP
    {                    ( () )}

defstack_same_expr:
  _1 = Int
    {                    ( () )}
| 
    {                    ( () )}

defstack:
  _1 = SEP
    {                        ( () )}

stack_map_frame_desc:
