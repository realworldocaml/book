in
      ( (name, Some k) )}

kind_and_name_in_value:
  _1 = LBrace name = LowerName _3 = RBrace
    {let name =
  let _endpos = _endpos_name_ in
  let _startpos = _startpos_name_ in
        ( (loc _startpos _endpos, `NewLowerName name) )
in
      ( (name, None) )}
