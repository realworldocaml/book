| 
    {                          ( ((fun t args -> NoOffset),

                             (fun off -> match off with
                                NoOffset -> Some []
                              | _ -> None))
                          )}
| _1 = DOT _2 = IDENT _3 = offset
    {                          ( ((fun t args ->
                                let fi = getField t _2 in
