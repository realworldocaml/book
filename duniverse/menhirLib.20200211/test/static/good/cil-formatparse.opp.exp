offset:
  _1 = ARG_o
    {                     ( let currentArg = _1 in
                            ((fun t args ->
                                match getArg currentArg args with
                                  Fo o -> o
                                | a -> wrongArgType currentArg "offset" a),

                              (fun off -> Some [ Fo off ]))
