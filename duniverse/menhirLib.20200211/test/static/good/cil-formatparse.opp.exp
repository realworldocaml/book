  _1 = type_spec _2 = attributes _3 = decl
    {                   ( ((fun args ->
                        let tal = (fst _2) args in
                        let ts = (fst _1) tal args in
                        let (fn, ft, _) = (fst _3) ts args in
                        (fn, ft, [])),

