                 mkMem e ((fst _5) baset args)),

                (fun l -> match l with
                  Mem e, off -> begin
                    match (snd _3) e, (snd _5 off) with
                      Some m1, Some m2 -> Some (m1 @ m2)
                    | _, _ -> None
