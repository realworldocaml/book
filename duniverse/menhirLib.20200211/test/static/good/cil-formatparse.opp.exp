                         | a -> wrongArgType currentArg "compinfo" a),

                        (fun t -> match unrollType t with
                            TComp(ci, _) -> Some [ Fc ci ]
                          | _ -> None))

                   )}
| _1 = TYPEOF _2 = LPAREN _3 = expression _4 = RPAREN
    {                   ( ((fun al args -> typeAddAttributes al
