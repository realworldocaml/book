                         match (snd _3) (fn, ft) with
                           Some (restt, m3) -> begin
                             match (snd _1) restt,
                                   (snd _2) (typeAttrs restt)with
                               Some m1, Some m2 ->
                                 Some (m1 @ m2 @ m3)
                             | _, _ -> None
                           end
                         | _ -> None))
