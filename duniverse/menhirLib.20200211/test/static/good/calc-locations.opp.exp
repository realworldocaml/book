%start main
%token DIV
%token EOL
%token <int> INT
%token LPAREN
%token MINUS
%token PLUS
%token RPAREN
%token TIMES
%left MINUS PLUS
%left DIV TIMES
%nonassoc UMINUS
%type <int> main
%%

main:
  _1 = expr _2 = EOL
    {                            ( _startpos__1_, _endpos__1_ )}

expr:
  _1 = INT
    {                            ( _1 )}
| _1 = LPAREN _2 = expr _3 = RPAREN
    {                            ( _2 )}
| _1 = expr _2 = PLUS _3 = expr
    {                            ( _1 + _3 )}
| _1 = expr _2 = MINUS _3 = expr
    {                            ( _1 - _3 )}
| _1 = expr _2 = TIMES _3 = expr
    {                            ( _1 * _3 )}
| _1 = expr _2 = DIV _3 = expr
    {                            ( _1 / _3 )}
| _1 = MINUS _2 = expr %prec UMINUS
    {                            ( - _2 )}

%%


