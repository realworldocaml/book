signature_fragment:
  _1 = RESOLVED frag = signature_resolved_fragment _3 = CLOSE
    {      ( Fragment.Resolved frag )}
| _1 = DOT md = signature_fragment data = Data _4 = CLOSE
    {      ( Fragment.Dot(md, data) )}

module_fragment:
  _1 = RESOLVED frag = module_resolved_fragment _3 = CLOSE
    {      ( Fragment.Resolved frag )}
| _1 = DOT md = signature_fragment data = Data _4 = CLOSE
