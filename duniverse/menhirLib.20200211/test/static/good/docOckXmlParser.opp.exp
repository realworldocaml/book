    {      ( data )}

flag_CONSTANT_:
  
    {      ( false )}
| _1 = CONSTANT _2 = CLOSE
    {      ( true )}

flag_INTERFACE_:
  
    {      ( false )}
| _1 = INTERFACE _2 = CLOSE
