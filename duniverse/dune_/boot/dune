(rule
 (mode promote)
 (action
  (copy ../bin/bootstrap-info libs.ml)))

(alias
 (name runtest)
 (deps libs.ml))

(executable
 (name duneboot)
 (modules :standard \ configure bootstrap)
 (libraries unix))

(executable
 (name bootstrap)
 (modules bootstrap))

;; For unused value warnings. We don't write a plain empty
;; duneboot.mli to simplify ../bootstrap.ml

(rule
 (with-stdout-to duneboot.mli (progn)))
