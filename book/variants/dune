(rule (with-stdout-to dune.gen
  (run ocaml-mdx rule --duniverse-mode README.md --direction=to-ml
                          --prelude=prelude.ml
                          --prelude=catch_all:catch_all.ml
                          --prelude=logger:logger.ml)))

(alias
 (name   runtest)
 (deps   README.md)
 (action (diff dune.inc dune.gen)))

(include dune.inc)
