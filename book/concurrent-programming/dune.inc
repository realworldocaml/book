(alias
 (name   runtest)
 (deps   (:x README.md)
         (package mdx)
         (:y8 ../../examples/code/async/better_echo.ml)
         (:y7 ../../examples/code/async/echo/echo.ml)
         (:y6 ../../examples/code/async/search/search.ml)
         (:y5 ../../examples/code/async/search_out_of_order.ml)
         (:y4 ../../examples/code/async/search_with_configurable_server/search_with_configurable_server.ml)
         (:y3 ../../examples/code/async/search_with_error_handling/search_with_error_handling.ml)
         (:y2 ../../examples/code/async/search_with_timeout.ml)
         (:y1 ../../examples/code/async/search_with_timeout_no_leak/search_with_timeout_no_leak.ml)
         (:y0 ../../examples/code/async/search_with_timeout_no_leak_simple/search_with_timeout_no_leak_simple.ml)
         (source_tree ../../examples/code/async/echo)
         (source_tree ../../examples/code/async/search)
         (source_tree ../../examples/code/async/search_with_configurable_server)
         (source_tree ../../examples/code/async/search_with_error_handling)
         (source_tree ../../examples/code/async/search_with_timeout_no_leak)
         prelude.ml)
 (action (progn
           (run ocaml-mdx test --prelude=prelude.ml --direction=to-ml %{x})
           (diff? %{y8} %{y8}.corrected)
           (diff? %{y7} %{y7}.corrected)
           (diff? %{y6} %{y6}.corrected)
           (diff? %{y5} %{y5}.corrected)
           (diff? %{y4} %{y4}.corrected)
           (diff? %{y3} %{y3}.corrected)
           (diff? %{y2} %{y2}.corrected)
           (diff? %{y1} %{y1}.corrected)
           (diff? %{y0} %{y0}.corrected)
           (diff? %{x} %{x}.corrected))))
(alias
 (name   runtest-all)
 (deps   (:x README.md)
         (package mdx)
         (:y8 ../../examples/code/async/better_echo.ml)
         (:y7 ../../examples/code/async/echo/echo.ml)
         (:y6 ../../examples/code/async/search/search.ml)
         (:y5 ../../examples/code/async/search_out_of_order.ml)
         (:y4 ../../examples/code/async/search_with_configurable_server/search_with_configurable_server.ml)
         (:y3 ../../examples/code/async/search_with_error_handling/search_with_error_handling.ml)
         (:y2 ../../examples/code/async/search_with_timeout.ml)
         (:y1 ../../examples/code/async/search_with_timeout_no_leak/search_with_timeout_no_leak.ml)
         (:y0 ../../examples/code/async/search_with_timeout_no_leak_simple/search_with_timeout_no_leak_simple.ml)
         (source_tree ../../examples/code/async/echo)
         (source_tree ../../examples/code/async/search)
         (source_tree ../../examples/code/async/search_with_configurable_server)
         (source_tree ../../examples/code/async/search_with_error_handling)
         (source_tree ../../examples/code/async/search_with_timeout_no_leak)
         prelude.ml)
 (action (progn
           (run ocaml-mdx test --prelude=prelude.ml --direction=to-ml --non-deterministic %{x})
           (diff? %{y8} %{y8}.corrected)
           (diff? %{y7} %{y7}.corrected)
           (diff? %{y6} %{y6}.corrected)
           (diff? %{y5} %{y5}.corrected)
           (diff? %{y4} %{y4}.corrected)
           (diff? %{y3} %{y3}.corrected)
           (diff? %{y2} %{y2}.corrected)
           (diff? %{y1} %{y1}.corrected)
           (diff? %{y0} %{y0}.corrected)
           (diff? %{x} %{x}.corrected))))
